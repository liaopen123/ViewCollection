<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activitys.FiveWayTimeCountActivity">
    <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
              app:layout_constraintTop_toTopOf="parent"
              app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintRight_toRightOf="parent"
              android:id="@+id/tv_handler" android:text="handler+postdelay 2 CountTime"/>

    <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintRight_toRightOf="parent"
              android:id="@+id/timerTask" android:text="timer+timerTask+handler 2 CountTime"
              android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/tv_handler"/>
    <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintRight_toRightOf="parent"
              android:id="@+id/ScheduledExecutorService" android:text="ScheduledExecutorService+handler 2 CountTime"
              android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/timerTask"/>

    <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintRight_toRightOf="parent"
              android:id="@+id/rxjava" android:text="Rxjava 2 CountTime" android:layout_marginBottom="8dp"
              android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/ScheduledExecutorService"/>
    <TextView android:layout_width="match_parent" android:layout_height="wrap_content"
              app:layout_constraintBottom_toBottomOf="parent"
              app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintRight_toRightOf="parent"
              android:id="@+id/countDownTimer" android:text="CountDownTimer 2 CountTime"
              android:layout_marginBottom="8dp"
              android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/ScheduledExecutorService"/>

</LinearLayout>